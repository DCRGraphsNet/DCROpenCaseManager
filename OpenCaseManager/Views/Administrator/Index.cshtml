
@{
    ViewBag.Title = "Administrator";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    #ui-datepicker-div {
        z-index: 9999 !important;
    }
</style>

<link rel="stylesheet" href="~/Scripts/plugins/Timepicker/jquery.timepicker.css" />
<script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
<script src="~/Scripts/plugins/Timepicker/jquery.timepicker.js"></script>
<link type="text/css" rel="stylesheet" href="~/Content/testcase.css" />
<!-- MultiSelect CSS & JS library -->
<link href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-multiselect/0.9.15/css/bootstrap-multiselect.css" rel="stylesheet" />
<script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-multiselect/0.9.15/js/bootstrap-multiselect.min.js"></script>


<div class="">
    <div class="create-form-groupedBtn-block">
        <div class="btn-group" name="create-form-label">
            <button id="buttonNewTestCase" type="button" data-toggle="modal" data-target="#addNewTestCase" class="btn groupedBtnStyling"><img src="../content/images/standard/addNewForm.png" class="newFormAddIcon" /><span data-locale data-apply="text" data-key="CreateNewTestCase">Create New Test Case</span> </button>
        </div>
        <div class="btn-group" name="create-form-label">
            <button id="buttonShowMyForms" type="button" class="btn groupedBtnStyling" style="display:none"><span data-locale data-apply="text" data-key="ShowMyForms">Show My Forms</span></button>
        </div>
    </div>

    <div id="myTestCasesLists" class="forms-listing-display-block">
        <h4 class="tasksHeading"><span data-locale data-apply="text" data-key="TestCases">Test Cases</span></h4>
        <table class="table table-striped forms-listing-table">
            <thead>
                <tr>
                    <th><span data-locale data-apply="text" data-key="Title">Title</span> </th>
                    <th><span data-locale data-apply="text" data-key="Created">Created</span> </th>
                    <th><span data-locale data-apply="text" data-key="Valid ">Valid</span> </th>
                    <th><span data-locale data-apply="text" data-key="NoOfTests"># Tests</span> </th>
                    <th><span data-locale data-apply="text" data-key="Actions">Actions</span> </th>
                </tr>
            </thead>
            <tbody id="myTestCases"></tbody>
        </table>
    </div>
</div>

<script src="~/Scripts/testCases.js"></script>
@Html.Partial("~/Views/PartialViews/AddTestCase.cshtml")
@Html.Partial("~/Views/PartialViews/ExportModal.cshtml")
@Html.Partial("~/Views/PartialViews/SendInvite.cshtml")
<script src="~/Scripts/jquery.treegrid.min.js"></script>



