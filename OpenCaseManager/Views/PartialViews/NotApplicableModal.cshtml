 <!--Modal For Commenting -->
<div class="modal fade" id="notApplicableModal" role="dialog">
    <div class="modal-dialog" style="top:20px">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title TasksWNoteheading">
                    <span data-locale data-apply="text" data-key="Title" id="notApplicableEventTitle">Title</span>
                </h4>
            </div>
            <div class="modal-body">
                <form>
                    <div class="form-group">
                        <label for="message-text" class="col-form-label" data-locale data-apply="text" data-key="Comment">Comment *</label>
                        <textarea class="form-control commentbox" id="message-text-not-applicable" rows="5"></textarea>
                    </div>
                </form>
            </div>
            <div id="activitySelected" style="display:none"></div>
            <div class="modal-footer">
                <button id="buttonNotApplicable" type="button" class="instanceAdd TasksWNoteBtn" data-locale data-apply="text" data-key="NotApplicable"></button>
            </div>
        </div>
    </div>
</div>
<!-- End Modal Commenting-->